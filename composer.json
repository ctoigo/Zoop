{
    "name": "ctoigo/acstoigo-zoop-sdk",
    "type": "library",
    "description": "API para integração com a ZOOP para boleto e cartão.",
    "keywords": [
        "libraries",
        "intermediary",
        "checkout",
        "marketplace",
        "zoop", 
        "sdk", 
        "toigo",
        "acstoigo",
        "ctoigo"
    ],
    "homepage": "https://github.com/ctoigo/acstoigo-zoop-sdk",
    "license": [
        "LGPL-3.0-or-later", 
        "GPL-3.0-or-later", 
        "MIT"
    ],
    "authors": [
        {
            "name": "Chrystian Toigo",
            "email": "ctoigo@gmail.com.br",
            "homepage": "https://www.acstoigo.com.br",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">= 7.0",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7",
        "phpstan/phpstan": "^0.9.2"
    },
    "autoload": {
        "psr-4": {
            "ACSToigo\\": "src/"
        } 
    },
    "autoload-dev": {
        "psr-4": {
            "ACSToigo\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "v1.0": "1.0-dev"
        }
    },
    "minimum-stability": "stable"
}
